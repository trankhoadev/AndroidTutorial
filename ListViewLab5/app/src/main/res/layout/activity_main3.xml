<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="@drawable/outter_background">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="360dp"
        android:layout_height="660dp"
        android:background="@drawable/inner_background"
        app:layout_constraintCircleRadius="10dp"
        tools:layout_editor_absoluteX="24dp"
        tools:layout_editor_absoluteY="43dp"
        tools:ignore="MissingConstraints">

        <TextView
            android:id="@+id/textView4"
            android:layout_width="210dp"
            android:layout_height="117dp"
            android:gravity="center|center_vertical"
            android:text="password generator"
            android:textAllCaps="true"
            android:textColor="#FFFFFF"
            android:textSize="35dp"
            tools:layout_editor_absoluteX="75dp"
            tools:layout_editor_absoluteY="27dp"
            tools:ignore="MissingConstraints" />

        <TextView
            android:id="@+id/textView7"
            android:layout_width="295dp"
            android:layout_height="50dp"
            android:background="#250247"
            android:textColor="#FFFFFF"
            tools:layout_editor_absoluteX="44dp"
            tools:layout_editor_absoluteY="193dp"
            tools:ignore="MissingConstraints" />

        <TextView
            android:id="@+id/textView8"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Password Length"
            android:textColor="#FFFFFF"
            tools:layout_editor_absoluteX="45dp"
            tools:layout_editor_absoluteY="287dp"
            tools:ignore="MissingConstraints" />

        <TextView
            android:id="@+id/textView10"
            android:layout_width="220dp"
            android:layout_height="40dp"
            android:gravity="center|start"
            android:text="Include lower case letter"
            android:textColor="#FFFFFF"
            android:textSize="18dp"
            tools:layout_editor_absoluteX="45dp"
            tools:layout_editor_absoluteY="328dp"
            tools:ignore="MissingConstraints" />

        <TextView
            android:id="@+id/textView14"
            android:layout_width="220dp"
            android:layout_height="40dp"
            android:gravity="center|start"
            android:text="Include special symbols"
            android:textColor="#FFFFFF"
            android:textSize="18dp"
            tools:layout_editor_absoluteX="45dp"
            tools:layout_editor_absoluteY="480dp"
            tools:ignore="MissingConstraints" />

        <TextView
            android:id="@+id/textView12"
            android:layout_width="220dp"
            android:layout_height="40dp"
            android:gravity="center|start"
            android:text="Include uppercase letter"
            android:textColor="#FFFFFF"
            android:textSize="18dp"
            tools:layout_editor_absoluteX="45dp"
            tools:layout_editor_absoluteY="385dp"
            tools:ignore="MissingConstraints" />

        <TextView
            android:id="@+id/textView13"
            android:layout_width="220dp"
            android:layout_height="40dp"
            android:gravity="center|start"
            android:text="Include number"
            android:textColor="#FFFFFF"
            android:textSize="18dp"
            tools:layout_editor_absoluteX="45dp"
            tools:layout_editor_absoluteY="433dp"
            tools:ignore="MissingConstraints" />

        <TextView
            android:id="@+id/textView11"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Password Length"
            android:textColor="#FFFFFF"
            tools:layout_editor_absoluteX="45dp"
            tools:layout_editor_absoluteY="287dp"
            tools:ignore="MissingConstraints" />

        <TextView
            android:id="@+id/textView9"
            android:layout_width="138dp"
            android:layout_height="40dp"
            android:background="#FFFFFF"
            tools:layout_editor_absoluteX="201dp"
            tools:layout_editor_absoluteY="277dp"
            tools:ignore="MissingConstraints" />

        <CheckBox
            android:id="@+id/checkBox"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:checked="true"
            app:buttonTint="#FFFFFF"
            tools:layout_editor_absoluteX="295dp"
            tools:layout_editor_absoluteY="328dp"
            tools:ignore="MissingConstraints" />

        <CheckBox
            android:id="@+id/checkBox2"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:checked="false"
            app:buttonTint="#FFFFFF"
            tools:layout_editor_absoluteX="294dp"
            tools:layout_editor_absoluteY="382dp"
            tools:ignore="MissingConstraints" />

        <CheckBox
            android:id="@+id/checkBox3"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:checked="true"
            app:buttonTint="#FFFFFF"
            tools:layout_editor_absoluteX="294dp"
            tools:layout_editor_absoluteY="432dp"
            tools:ignore="MissingConstraints" />

        <CheckBox
            android:id="@+id/checkBox4"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:checked="false"
            app:buttonTint="#FFFFFF"
            tools:layout_editor_absoluteX="294dp"
            tools:layout_editor_absoluteY="480dp"
            tools:ignore="MissingConstraints" />

        <Button
            android:id="@+id/button3"
            android:layout_width="285dp"
            android:layout_height="60dp"
            android:text="generate password"
            app:backgroundTint="#5332E4"
            tools:layout_editor_absoluteX="38dp"
            tools:layout_editor_absoluteY="560dp"
            tools:ignore="MissingConstraints" />

    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.constraintlayout.widget.ConstraintLayout>